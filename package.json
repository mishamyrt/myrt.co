{
  "name": "myrt.co",
  "private": true,
  "dependencies": {
    "parcel": "^1.12.4",
    "sass": "^1.26.10",
    "pug": "^3.0.0"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.2",
    "stylelint": "^13.7.1",
    "stylelint-config-standard": "^20.0.0",
    "yaspeller": "^7.0.0"
  },
  "scripts": {
    "build": "npx parcel build src/index.pug",
    "build:dev": "npx parcel build src/index.pug --no-source-maps --no-minify --log-level 0",
    "start": "npx parcel src/index.pug",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npm run lint:css",
    "lint:css": "npx stylelint \"src/**/*.scss\"",
    "spellcheck": "npm run build:dev && npx yaspeller dist/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mishamyrt/myrt.co.git"
  },
  "author": "mishamyrt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mishamyrt/myrt.co/issues"
  },
